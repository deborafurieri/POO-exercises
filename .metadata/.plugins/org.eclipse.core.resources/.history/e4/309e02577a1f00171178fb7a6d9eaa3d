package banco;
import java.util.ArrayList;

public class BancoImpl{
	public static void main(String[] args) {

		ContaCorrente c1 = new ContaCorrente();
		ContaCorrente c2 = new ContaCorrente();
		ContaCorrente c3 = new ContaCorrente();
		
		ArrayList<ContaCorrente> conta = new ArrayList<>();
		conta.add(c1);
		conta.add(c3);
		conta.add(c2);
		
		c1.setNome("Joao");
		c2.setNome("Jose");
		c3.setNome("Maria");
		
		c1.setNumero("123-4");
		c2.setNumero("123-5");
		c3.setNumero("123-6");
		
		c1.setLimite(1000);
		c2.setLimite(2000);
		c3.setLimite(3000);
		
		c1.setSaldo(2000);
		((Banco) c1).deposito(200);
		
		
	}
}
